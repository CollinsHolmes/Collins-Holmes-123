def jason_peek(weight,height,skin_tone,hair_color):    jason = {        'Weight':weight,        'Height':height,        'Skin_tone':skin_tone,        'Hair_color':hair_color,        }    return jason         jp = jason_peek ('120','5"9', 'White','Dirty Blonde')
def display_jason(jason):    for key, value in jason.items():        print(key + " : "+ value)        print ("\n")        display_jason(jp)
